  "devDependencies": {
    "@sveltejs/kit": "next",
    "svelte": "^3.44.0",
    "svelte-adapter-bun": "^0.3.1",
    "svelte-check": "^2.8.0",
    "svelte-preprocess": "^4.10.7",
    "typescript": "^4.7.4",
    "tslib": "^2.3.1",
    "vite": "^3.0.0"
  },
  "type": "module",
  "dependencies": {
    "bun-types": "^0.1.4"
  }
}